<body style="background-color: #f9fafa;">
	<div id="wrapper" class="page-center"><!--  data-type="profile"
		data-pre="alarm" data-color="M"
		style="margin-right: 0px; margin-left: 0px;" -->
		<div class="intro">
			<img src="../images/load_015s.gif" alt="" />
		</div>
		<!-- header -->
		<!-- <header id="header_new" style="margin-right: 0px; margin-left: 0px;">
			top-area
			<div class="top-area" id="navbar">
				<h1>个人信息</h1>

			</div>
			//top-area


		</header> -->
		<!-- //header -->


		<!-- contain -->
		<div id="container"  style="display: none;">
			<!-- 编码区 -->
			<section class="pr_edit">

				<div class="row user">
					<span class="thum"> <img class="edit_profile_img"
						src="../images/common/avatar.gif" alt="">
					</span>

					<dl>
						<!-- <dt>
							<p id="edit_teacher_name"></p>
							<p>
								<span class="edit_class_number"></span>
							</p>
						</dt> -->
						<dd>
							<a href="javascript:void(0)"
								class="btn-block btn_edit_profile_img" data-kor="프로필 사진 바꾸기"
								style="display: none;">更换头像</a>
						</dd>
					</dl>
				</div>

				<!-- 输入表单 -->
				<form name="fmEdit" id="fmEdit" enctype="multipart/form-data"
					method="post" onsubmit="return false;">
					<div class="row info">
						<ul>
							<li><strong data-kor="用户名">用户名</strong>
								<p>
									<span id="edit_user_id1" class="edit_user_id"></span>
								</p></li>
							<li><strong data-kor="民族">民族</strong>
								<p>
									<span class="edit_user_national"></span>
								</p> <strong data-kor="籍贯">籍贯</strong>
								<p>
									<span class="edit_user_originate"></span>
								</p></li>

							<li><strong data-kor="出生日期">出生日期</strong>
								<p>
								<p>
									<span class="edit_user_birthday"></span>
								</p>
								</p></li>

							<li><strong data-kor="性别">性别</strong>
								<p>
									<input id="male" type="radio"
										class="search_app_student_rdo_sex male" name="sex" value="M">
									<label for="" data-kor="男">男</label> <input id="female"
										type="radio" class="search_app_student_rdo_sex female"
										name="sex" value="F"> <label for="" data-kor="女">女</label>
								</p></li>

							<li><strong data-kor="地址">地址</strong>
								<p>
									<input type="text" id="address" class="control address" />
								</p></li>

							<li><strong data-kor="联系电话">联系电话</strong>
								<p>
									<input type="text" class="control phone phone01" maxlength="11" />
								</p></li>

							<li><strong data-kor="邮箱">邮箱</strong>
								<p>
									<input type="text" id="email" class="control email01" />
								</p></li>

							<li><strong data-kor="QQ">QQ</strong>
								<p>
									<input type="text" name="QQ" class="control1" />
								</p></li>

							<li><strong data-kor="教龄">教龄</strong>
								<p>
									<span class="edit_user_teacher_year"></span>
								</p></li>

							<li><strong data-kor="职称">职称</strong>
								<p>
									<span class="edit_user_teacher_position"></span>
								</p> <strong data-kor="职务">职务</strong>
								<p>
									<span class="edit_user_teacher_duty"></span>
								</p></li>
						</ul>
					</div>
				</form>
				<!-- //입력폼 -->
				<div class="group-btn">
					<a href="javascript:void(0)" class="btn-round danger btn_edit_teacher" data-kor="保存修改">保存修改</a>
				</div>
				<div id="menuDim"
					style="position: absolute; min-width: 320px; min-height: 100%; top: 50px; z-index: 100; display: none;"></div>
				<!-- //menu Dim -->

				<!-- alert -->
				<div class="alert" id="main_common_alert" style="display: none;">

				</div>
				<!-- //alert -->

				<!-- <p class="desc" data-kor="欢迎进入暖心妈妈微信端"
					style="padding: 0; font-size: 14px;">
					<strong> 欢迎使用 </strong> <strong class="app"> 暖心微信端 </strong>
				</p> -->
			</section>
			<!-- //코딩 영역 -->


			<!-- <script src="../js/weixin/profile.js"></script> -->
		</div>
	</div>


