<style>
	cts-fisrt-view{
		position:absolute;
		display:block;
		width:100%;
		height:100%;
		background: white;
		top:0;
		left:0;
		display:flex;
		align-items: center;
		text-align: center;
		z-index: 9;
	}
	cts-fisrt-view > *{
		flex:1;
		display:flex;
		text-align: center;
		flex-direction: column;
	}
	cts-loading{
		position:absolute;
		display:block;
		width:100%;
		height:100%;
		background:rgba(255,255,255,.7);
		top:0;
		left:0;
		display:flex;
		align-items: center;
		text-align: center;
		 z-index: 9;
	}
	/* Loading */
	@keyframes ctsloadingrotate{
		0% {-webkit-transform: rotate(0deg) translate3d(0,0,0)}
		100% {-webkit-transform: rotate(359deg) translate3d(0,0,0)}
	}
	@-webkit-keyframes ctsloadingrotate {
		0% {-webkit-transform: rotate(0deg) translate3d(0,0,0)}
		100% {-webkit-transform: rotate(359deg) translate3d(0,0,0)}
	}
	.graph_area{position: relative; min-height: 150px;}
	.cts-r-header > h4{ display:flex; height: initial;}
	.cts-r-header > h4 > *{ display:flex; }
	.cts-r-header .cts-r-h-box-area{flex:1;}
	.cts-r-header .cts-r-h-box-area [cts-r-h-optbox-area]{
		width:100%;
	}
	.cts-r-header .cts-r-h-box-area [cts-r-h-optbox-area] > .control.form-control{
		display:block; max-width:initial;
		width: calc(100% - 50px);
		margin:5px 0;
	}
	.cts-r-header div[cts-r-h-classlistbox-area]{display:none !important;}
	/*.cts-r-header div[cts-r-h-optbox-area] select:not(:last-child){margin-left:10px;}*/
	.cts-r-header div.cts-r-h-button-area {  flex: none; text-align: right; align-items: center;justify-content: flex-end;}
	div[cts-content-chart-area]{ margin-top: 15px; }
	.chart_icon {font-size: 0;}
	.chart_icon a {
		display: inline-block; width: 26px; height: 25px; margin: 0 5px 0 0;
		background-image: url('./report_req/ico_type.gif'); background-repeat: no-repeat;
		-webkit-filter: grayscale(100%);
		display:none;
	}
	.chart_icon a[cts-chart-build=bar] {background-position: calc(-30px*0) 0;}
	.chart_icon a[cts-chart-build=line] {background-position: calc(-30px*1) 0;}
	.chart_icon a[cts-chart-build=spider] {background-position: calc(-30px*2) 0;}
	.chart_icon a[cts-chart-build=pie] {background-position: calc(-30px*3) 0;}
	.chart_icon a:hover,
	.chart_icon a.on {-webkit-filter: grayscale(0%);}


	table.cts-content-table { 
		/*width: 100%; margin: -1px 0 0 0;*/
		width:98%;
		margin: 10px auto 10px;
		border-radius: 15px;
		font-size:13px; 
		border : 1px solid #e2e2e2;
	}
	table.cts-content-table th,
	table.cts-content-table td {
		border-top: 1px solid #e2e2e2; text-align: center; position: relative; 
	}
	table.cts-content-table.getPhysicalTestStatTable{
		margin-bottom:25px;
		font-size:12px !important;
	}

	table.cts-content-table.getPhysicalTestStatTable th,
	table.cts-content-table.getPhysicalTestStatTable td {
		border-bottom: 1px solid #e2e2e2; text-align: center; position: relative;
	}
	
	table.cts-content-table th, table.cts-content-table td  {
		height: 34px; vertical-align: middle; 
	}
	table.cts-content-table th{ 
		background-color:#f0f0f0;
		font-weight: bold; 
	}
	table.cts-content-table th.cts-content-table-no{
		width:30px; 
	}
	table.cts-content-table .cts-color-label{
		display:inline-block;
		width:11px;height:11px;
		border-radius:5px;
		border:1px solid rgba(255,255,255,1);
		background:#eaeaea;
	}
	table.cts-content-table .ct-data-summary-tr td,table.cts-content-table .ct-data-summary-tr th{
		background : #F0F0F0;
	}
	table.cts-content-table .ct-data-summary-tr .cts-color-label {
		height:2px;
		border:none;
	}
	/* 활성화 시 */ 
	table.cts-content-table .active td {
		background: #ffffcc;
		color: black;
	}


	[cts-table-sort]:after{
		display: inline;
		content : "↕";
		color: gray;
	}
	[cts-table-sort=up]:after{
		content:"↑";
		color : red;
	}
	[cts-table-sort=down]:after{
		content:"↓";
		color : blue;
	}
	.getPhysicalTestStatHeader{
		padding : 15px 0;
		text-align: center;
		font-size : 15px;
		font-weight: bold;
	}
	.getPhysicalTestStatHeader td{
		vertical-align: middle;
		text-align: center;
	}
	.control.form-control{
		/*max-width: 80px;*/
		width: calc(100% - 50px);
	}
	span.control{
		/*height: 26px;*/
		line-height: 0;
		color: #000;
	}
	span.control .control.form-control{
		width:100%;
	}

	[diffarea]{
		  margin: 10px;
	}
	[diffarea] .colorLabel{
		display:inline-block;
		width:30px;
		height:30px;
		border-radius: 15px;
		position: relative;
		margin-left:20px;
		margin-right:5px;
	}
	[diffarea] .colorLabel > span:first-child{
		content: "";
		position:absolute;
		width:30px;
		height:4px;
		top:13px;
		left:0;
	}
	[diffarea] .colorLabel > span:last-child{
		content: "";
		width:15px;
		height:15px;
		border-radius: 15px;
		position:absolute;
		top: calc(15px - 7px);
		left: calc(15px - 7px);
	}
	[diffarea] select{
	-webkit-appearance: none;
	font-size: 13px;
	border: none;
	line-height: 26px;
	padding: 0 25px 0 15px;
	border: 1px solid #e1e1e1;
	color: #000;
	}

	.chart_icon {font-size: 0;}
	.chart_icon li {
		display: inline-block; width: 26px; height: 25px; margin: 0 5px 0 0;
		background-image: url('./report_req/ico_type.gif'); background-repeat: no-repeat;
		-webkit-filter: grayscale(100%);
		display:none;
		float:right;
		margin: 2px 5px 0 5px;
	}
	.chart_icon li[cts-chart-build=bar] {background-position: calc(-30px*0) 0;}
	.chart_icon li[cts-chart-build=line] {background-position: calc(-30px*1) 0;}
	.chart_icon li[cts-chart-build=spider] {background-position: calc(-30px*2) 0;}
	.chart_icon li[cts-chart-build=pie] {background-position: calc(-30px*3) 0;}
	.chart_icon li:hover,
	.chart_icon li.on {-webkit-filter: grayscale(0%);}
</style>


<!-- 코딩 영역 -->
<section class="statistic cts-r-containor">


		<div class="cts-r-header group_select">
			<h4>
				<!-- select 영역 -->
				<div class="cts-r-h-box-area" cts-r-h-area>
					<div cts-r-h-optbox-area></div>
					<div cts-r-h-classlistbox-area></div>
				</div>


				<div class="cts-r-h-button-area">
					<button cts-build-report class="btn-round danger btn_search" id="makeChart" data-kor="조회">查询</button>
					<!-- <button cts-build-report style="display:none;">조회</button> -->
				</div>
				
			</h4>

			<article class="cts-r-content graph_area">
				<cts-loading>
				</cts-loading>
				<cts-fisrt-view>
					<span>
						请先设置统计条件后，<br/>
						点击查询进行查看。
					</span>
				</cts-fisrt-view>
				<div class="cts-content-chart-area" id="container">
					<ul class="cts-r-h-button-area chart_icon">
						<li href="javscript:void(0);" cts-chart-build="line"></li>
						<li href="javscript:void(0);" cts-chart-build="bar"></li>
						<li href="javscript:void(0);" cts-chart-build="pie"></li>
					</ul>
					<div id="cts-content-chart-area" cts-content-chart-area></div>
				</div>
				<div class="cts-content-table-area" cts-content-table-area>
				</div>
			</article>


			<!-- //학부모 피드백 -->

		</div>

</section>
