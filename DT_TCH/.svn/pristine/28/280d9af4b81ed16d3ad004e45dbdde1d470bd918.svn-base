<body>
	<div id="wrapper">

		<!-- contain -->
		<div id="container">

			<!-- 编码区 -->
			<section class="t-notice">
				<header data-am-widget="header" class="am-header am-header-default">
					<h1 class="am-header-left am-header-nav">
						<label id="txtTeacherStatus"> 家校通 </label>
					</h1>

					<div class="am-header-right am-header-nav" style="display: none;">
						<a href="#doc-oc-demo2" href="#doc-oc-demo1" data-am-offcanvas>
							切换 </a>
					</div>
				</header>

				<!-- 侧边栏内容 -->
				<div id="doc-oc-demo2" class="am-offcanvas">
					<div class="am-offcanvas-bar">
						<section
							class="amz-sidebar am-offcanvas-bar am-offcanvas-bar-overlay am-offcanvas-bar-active">



							<ul id="selTeacherStatus" class="am-nav">

							</ul>
						</section>
					</div>
				</div>

				<div class="view">
					<!-- view header -->
					<table style="width: 100%;">
						<tr>
							<td rowspan="2"><dt class="view_title"></dt></td>
							<td align="right" id="td2"></td>
						</tr>
						<tr>
							<td align="right" id="td3"></td>
						</tr>
					</table>
				</div>
				<div class="div-divider"></div>
				<div class="view">
					<dd>
						<a href="javascript:void(0)" class="link view_writer"></a>
					</dd>
					<hr width="80%" />
					<dd>
						<a href="javascript:void(0)" class="link view_alarm"></a>
					</dd>
				</div>

				<div class="div-divider"></div>

				<!-- //view header -->

				<!-- view-area -->
				<div class="view cnt-contain">
					<div class="cnt"></div>
				</div>
				<!-- //view-area -->
			</section>
		</div>
		<!-- //编码区 -->
	</div>
	<!-- //contain -->


	<!-- alert -->
	<div class="alert" id="main_common_alert" style="display: none;">
	</div>

	<!-- alert -->
	<div class="alert" id="main_common_balert" style="display: none;">
	</div>

	<!-- //alert -->
	</div>

	<a href="javascript:void(0)" class="top"> </a>


	<!-- LOADING -->
	<div id="loadingWrap" class="loadwrap" style="display: none;">
		<div class="pace pace-active">
			<div class="pace-progress" data-progress="50"
				data-progress-text="50%">
				<div class="pace-progress-inner"></div>
			</div>
			<div class="pace-activity"></div>
		</div>
	</div>

	<script type="text/javascript" src="../js/weixin/alarm_view.js">
		
	</script>

