
package com.digital.tch.common.vo;

import javax.xml.bind.annotation.XmlElement;
import javax.xml.bind.annotation.XmlRootElement;
import javax.xml.bind.annotation.XmlType;

import com.digital.tch.common.util.CommonUtil;

@XmlRootElement(name = "record")
@XmlType(name = "board", namespace = "com.visangesl.vivasam.author.vo.Board", propOrder = { "idx", "title", "content", "memo" })
public class Board {
    int idx;
    String title;
    String content;
    String memo;
	
    public int getIdx() {
	return idx;
    }
	
    @XmlElement(name = "idx")
    public void setIdx(int idx) {
	this.idx = idx;
    }
	
    public String getTitle() {
	return title;
    }
	
    @XmlElement(name = "title")
    public void setTitle(String title) {
	this.title = title;
    }

    public String getContent() {
	return content;
    }
	
    @XmlElement(name = "content")
    public void setContent(String content) {
	this.content = content;
    }
	
    public String getMemo() {
	return memo;
    }
	
    @XmlElement(name = "memo")
    public void setMemo(String memo) {
	this.memo = memo;
    }

    @Override
    public String toString() {
        // TODO Auto-generated method stub
        StringBuffer sb = new StringBuffer();
        sb.append(idx);
        sb.append(CommonUtil.colsep);
        sb.append(title);
        sb.append(CommonUtil.colsep);
        sb.append(content);
        sb.append(CommonUtil.colsep);
        sb.append(memo);
        
        return sb.toString();
    }
	 
	
}
