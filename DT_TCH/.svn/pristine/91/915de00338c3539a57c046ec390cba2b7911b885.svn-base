/* Reset */
* {
	box-sizing: border-box;
}

html, body {
	width: 100%;
	height: 100%;
}

body {
	line-height: 1.4;
	font-family: tahoma, Helvetica, sans-serif !important;
	font-size: 14px;
	color: #333;
	overflow-x: hidden;
	/* -webkit-user-select: none; */
	-webkit-tap-highlight-color: transparent;
}

iframe {
	border: 0;
}

h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
}

html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address,
big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt,
var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside,
canvas, details, embed, figure, figcaption, footer, header, hgroup, menu, nav, output, ruby, section, summary, time, mark, audio, video, input, select, textarea {
	/* margin: 0; */
	padding: 0;
	border: 0;
}

legend {
	display: none;
}

mark {
	background: none;
}

/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {
	display: block;
}

img {
	border: none;
	max-width: 100%;
}

i, em {
	font-style: normal;
}

ol, ul {
	list-style: none;
}

table {
	border-collapse: collapse;
	border-spacing: 0;
	table-layout: fixed;
}

a {
	text-decoration: none;
	color: #fff;
}

input, select, textarea {
	-webkit-border-radius: 0;
	outline: none;
}

button {
	border: 0;
}

#wrapper {
	min-width: 320px;
}

#wrapper #content_container {padding: 5px 0 0 0;}
#wrapper.sub-show #content_container {padding: 121px 0 0 0;}
#wrapper.sub-view #content_container {padding: 0;}
#wrapper.sub-show #header {height: 123px;}

#header {
	overflow: hidden;
	height: 88px;
	transition: 0.7s;
	box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.2);
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 10;
	background-color: #fff;
}
#header .top-area {
	display: -webkit-box;
	-webkit-box-pack: center;
	-webkit-box-align: center;
	position: relative;
	height: 53px;
}
#header .top-area .logo {
	display: block;
	width: 189.5px;
	height: 28px;
	font-size: 0;
}
#header .top-area .btn-menu {
	display: inline-block;
	position: absolute;
	left: 9px;
	top: 9px;
	width: 36px;
	height: 36px;
	border-radius: 4px;
	border: 1px solid #c7c7c7;
	background: linear-gradient(#ffffff, #e7e7e7);
}
#header:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}

#header nav {
	position: relative;
	height: 36px;
	background-color: #f46758;
}
#header nav > .dep1 {
	display: -webkit-box;
	-webkit-box-pack: center;
}
#header nav > .dep1 > li {
	width: 24%;
	padding: 2px 0 0 0;
	letter-spacing: -1px;
}

#header nav > .dep1 > li > a * {vertical-align: middle;}
#header nav > .dep1 > li > a {
	display: block;
	height: 35px;
	text-align: center;
	line-height: 35px;
	font-size: 16px;
	color: #fff;
}

@media screen and (max-width:480px) {
	#header nav > .dep1 > li > a { font-size: 14px; }
}


#header nav > .dep1 > li > a.on {
	color: #f46758;
	background-color: #fff;
}

#header nav > .dep1 > li > a.on i {
	background-color: #f46758;
	color: #fff;
}

#header nav .dep2 {
	-webkit-box-pack: center;
	position: absolute;
	top: 35px;
	left: 0;
	width: 100%;
	height: 35px;
	background-color: #fff;
}
#header nav .dep2.on {
	display: -webkit-box;
}
#header nav .dep2 > li {
	position: relative;
	width: 24%;
}
#header nav .dep2 > li:before {
	content: '';
	position: absolute;
	top: 50%;
	left: 0;
	margin-top: -5px;
	display: block;
	height: 10px;
	border-left: 1px solid #959595;
}
#header nav .dep2 > li:first-child:before {
	display: none;
}
#header nav .dep2 > li > a {
	display: block;
	height: 35px;
	text-align: center;
	line-height: 35px;
	font-size: 16px;
	color: #000;
}
#header nav .dep2 > li > a.on {
	color: #f46758;
}
