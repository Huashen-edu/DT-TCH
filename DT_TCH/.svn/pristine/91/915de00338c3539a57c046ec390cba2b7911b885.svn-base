/* ================ header_new ================ */
#header_new {
	overflow: hidden; position: fixed; top: 0; left: 0; width: 100%; height: 88px; z-index: 10; background-color: #fff;
	-webkit-transition: 0.7s; box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.2);
}
#header_new .top-area {display: -webkit-box; -webkit-box-pack: center; -webkit-box-align: center; position: relative; height: 53px;}
#header_new .top-area .logo {display: block; width: 189.5px; height: 28px; font-size: 0;}
#header_new .top-area .btn-menu {
	display: inline-block; position: absolute; left: 9px; top: 9px; width: 36px; height: 36px;
	border-radius: 4px; border: 1px solid #c7c7c7; background: linear-gradient(#ffffff, #e7e7e7);
}
#header_new:after {content: ''; display: block; height: 0; visibility: hidden; clear: both;}
#header_new nav {
	overflow: hidden; overflow-x: scroll;
	position: relative; width: 100%; height: 36px;
}

/* 아빠 */
#wrapper[data-color="D"] #header_new nav {
	border-top: 2px solid #0d10e7;
}
/* 엄마 */
#wrapper[data-color="M"] #header_new nav {
	border-top: 2px solid #f46758;
}
/* 기타 */
#wrapper[data-color="E"] #header_new nav {
	border-top: 2px solid #05ac38;
}

#header_new nav::-webkit-scrollbar {height: 0;}
#header_new nav > .dep1 {display: flex; display: -webkit-flex;}
#header_new nav > .dep1 > li {padding: 0 1.5%; flex-grow:1; -webkit-flex-grow:1;}
#header_new nav > .dep1 > li > a * {vertical-align: middle;}
#header_new nav > .dep1 > li > a {
	display: block; height: 35px; line-height: 35px; padding: 0 4px; text-align: center; font-size: 16px; color: #333;
	-webkit-box-sizing: border-box; white-space: nowrap;
}

#header_new nav > .dep1 > li > a.on {height: 33px;}

/* 아빠 */
#wrapper[data-color="D"] #header_new nav > .dep1 > li > a.on {
	color: #0d10e7; border-bottom: 2px solid #0d10e7;
}
/* 엄마 */
#wrapper[data-color="M"] #header_new nav > .dep1 > li > a.on {
	color: #f46758; border-bottom: 2px solid #f46758;
}
/* 기타 */
#wrapper[data-color="E"] #header_new nav > .dep1 > li > a.on {
	color: #05ac38; border-bottom: 2px solid #05ac38;
}

/* 폴링 카운트 */
#header_new nav > .dep1 > li > a .cts_count {
	box-sizing: border-box;
	position: relative;
	top: -1px;
	display: none;
	min-width: 16px;
	height: 16px;
	line-height: 16px;
	margin-left: 4px;
	padding: 0 4px;
	background-color: #f46758;
	color : #fff;
	border-radius: 8px;
	font-size: 12px;
}

#header_new nav > .dep1 > li > a .cts_count.pollingOn {
	display: inline-block;
}