@charset "UTF-8";
.fc_danger {
	color: #f46758 !important;
}

.fc_active {
	color: #2eb8c5 !important;
}

/* input */
.textarea {
	width: 100%;
	height: 240px;
	padding: 10px;
	font-size: 15px;
	box-shadow: inset 0 0 10px 1px rgba(0, 0, 0, 0.2);
	resize: none;
	border-radius: 5px;
}

select.control {
	height: 26px;
	line-height: 26px;
	border-radius: 6px;
	padding: 0 10px;
	border: 1px solid #e5e5e5;
	color: #000;
}

input[type="text"].control {
	height: 26px;
	line-height: 26px;
	border-radius: 6px;
	padding: 0 10px;
	border: 1px solid #e5e5e5;
	color: #000;
}

input[type="password"].control {
	height: 26px;
	line-height: 26px;
	border-radius: 6px;
	padding: 0 10px;
	border: 1px solid #e5e5e5;
	color: #000;
}

input[type="phone"].control {
	height: 26px;
	line-height: 26px;
	border-radius: 6px;
	padding: 0 10px;
	border: 1px solid #e5e5e5;
	color: #000;
}

input[type="checkbox"] {
	-webkit-appearance: none;
	position: relative;
	display: inline-block;
	width: 18px;
	height: 18px;
	border: 1px solid #959595;
	background-color: #ccc;
}
input[type="checkbox"]:checked:after {
	content: '';
	display: block;
	position: absolute;
	top: 2px;
	left: 2px;
	width: 12px;
	height: 12px;
	background-color: #f46758;
}

/* button_round */
.group-btn {
	text-align: center;
}
.group-btn.left {
	text-align: left;
}
.group-btn.right {
	text-align: right;
}

.btn-round * {
	vertical-align: middle;
}

.btn-round.full {
	display: block;
	width: 80%;
	margin: 0 auto;
}

.btn-round {
	display: inline-block;
	height: 40px;
	line-height: 40px;
	border-radius: 40px;
	background: linear-gradient(#c7c7c7, #aeaeae);
	box-shadow: inset 0 1px 0px 0 rgba(255, 255, 255, 0.6), 0 2px 3px 0 rgba(0, 0, 0, 0.2);
	padding: 0 25px;
	text-align: center;
	color: #fff;
	font-size: 20px;
	border: 1px solid #949494;
}

.btn-round.danger {
	background: linear-gradient(#f46757, #da4d3e);
	border: 1px solid #c61300;
}

/* 아빠 */
#wrapper[data-color="D"] .btn-round.danger {
	background: linear-gradient(#0d10e7, #393aea);
	border: 1px solid #0111ac;
}
/* 엄마 */
#wrapper[data-color="M"] .btn-round.danger {
	background: linear-gradient(#f46757, #da4d3e);
	border: 1px solid #c61300;
}
/* 기타 */
#wrapper[data-color="E"] .btn-round.danger {
	background: linear-gradient(#03bb3f, #22ba4d);
	border: 1px solid #266133;
}

.btn-round.half {width: 45%;}

/* button_block */
.btn-block {
	display: inline-block;
	min-width: 62px;
	padding: 0 8px;
	height: 25px;
	line-height: 25px;
	border-radius: 6px;
	font-size: 15px;
	color: #505050;
	border: 1px solid #ccc;
	text-align: center;
	background-color: #f0f2f3;
}

.page-center {
	display: -webkit-box;
	-webkit-box-align: center;
	-webkit-box-pack: center;
}

.board-header {
	height: 35px;
	background: linear-gradient(#ffffff, #fff);
	padding: 5px 15px 0 15px;
	text-align: right;
}

/* alert */
.alert {
	z-index:99999;
	overflow: hidden;
	position: fixed;
	top: 40%;
	left: 50%;
	width: 300px;
	margin: -75px 0 0 -150px;
	border-radius: 20px;
	border: 2px solid #d7d7d7;
	font-size: 20px;
	background-color: #fff;
	box-shadow: 4px 4px 10px 0 rgba(0, 0, 0, 0.4);
}

.alert .header{padding: 14px; font-weight: bold;}

.alert .desc {
	display: -webkit-box;
	-webkit-box-align: center;
	-webkit-box-pack: center;
	text-align: center;
	min-height: 79px;
	padding: 14px;
	box-shadow: 0 0 10px 0 rgba(0, 0, 0, 0.2);
	font-size: 18px;
}

/* 아빠 */
#wrapper[data-color="D"] .alert .desc strong {
	color: #0d10e7;
}
/* 엄마 */
#wrapper[data-color="M"] .alert .desc strong {
	color: #f46758;
}
/* 기타 */
#wrapper[data-color="E"] .alert .desc strong {
	color: #05ac38;
}

.alert .group-btn {padding: 15px 0; text-align: center;}
.alert .block {display: block !important;}
.alert .num-box {display: block; height: 40px; line-height: 40px; margin-bottom: 15px; font-size: 24px; background-color: #eaeaea;}
.alert .check-id {margin: 7px 0;}
.alert .check-id .control-login {color: #666;}
.alert .fail {font-size: 14px;}

.alert-alpha {
	z-index:99999;
	overflow: hidden;
	position: fixed;
	top: 50%;
	left: 50%;
	width: 400px;
	margin: -45px 0 0 -200px;
	padding: 15px;
	border-radius: 20px;
	border: 2px solid #d7d7d7;
	font-size: 20px;
	text-align: center;
	background-color: rgba(0,0,0,.6);
	box-shadow: 4px 4px 10px 0 rgba(0, 0, 0, 0.4);
	color: #fff;
}

/* side-info */
.side-info {
	position: absolute;
	top: 0;
	left: 0;
	color: #fff;
	width: 258px;
	min-height: 100%;
	background: #323232;
}
.side-info header {
	background: linear-gradient(#32325b, #28284f);
}
/* 기타 */
/*
#wrapper[data-color="E"] .side-info header {
	background: linear-gradient(#06492a, #044429);
}
*/

.side-info .row {
	padding: 6px 13px;
	border-bottom: 1px solid #3a3b5c;
	box-shadow: inset 0 -1px 0 0 #1f1d1f;
}
.side-info .btn-round-line.login {
	display: inline-block;
	height: 27px;
	line-height: 27px;
	border-radius: 27px;
	padding: 0 15px;
	text-align: center;
	border: 2px solid #636391;
	color: #fff;
	background-color: #28284f;
}
/* 기타 */
#wrapper[data-color="E"] .side-info .btn-round-line.login {
	background-color: #044429;
	border: 2px solid #2b8359;
}

.side-info .user * {
	vertical-align: middle;
}
.side-info .user .thum {
	float: left;
	overflow: hidden;
	display: inline-block;
	margin-right: 12px;
	width: 102px;
	height: 102px;
	border-radius: 50%;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
}
.side-info .user dl {
	overflow: hidden;
}
.side-info .user dl dt strong {
	font-size: 27px;
}
.side-info .user dl dt p * {
	vertical-align: baseline;
}
.side-info .user dl dt p {
	margin: 0 0 0 0;
	color: #9090c5;
	font-size: 12px;
	font-weight: bold;
}
/* 기타 */
#wrapper[data-color="E"] .side-info .user dl dt p {
	color: #55cb97;
}

.side-info .user dl dt p span {
	color: #fff;
}
.side-info .user dl dd {
	margin-top: 15px;
	text-align: center;
}
.side-info .user:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
.side-info .info ul li {
	width: 100%;
	display: table;
	padding: 12px 14px;
	border-bottom: 1px solid #454545;
	box-shadow: inset 0 -1px 0 0 #1f1d1f;
	font-size: 12px;
}
.side-info .info ul li strong,
.side-info .info ul li p {
	display: table-cell;
	vertical-align: middle;
}
.side-info .info ul li strong {
	width: 60px;
	color: #9090c5;
}

/* 기타 */
#wrapper[data-color="E"] .side-info .info ul li strong {
	color: #4cfe8e;
}

.side-info .info ul li p {
	font-size: 14px;
	word-break: break-all;
}
.side-info footer {
	padding: 18px 20px;
	text-align: right;
}
.side-info footer .setting {
	display: inline-block;
	height: 24px;
	line-height: 24px;
	padding: 0 0 0 33px;
	background: url("../images/common/icon/ico_set.png") 0 0 no-repeat;
	background-size: 24px 24px;
	color: #fff;
}

.list-area {
	padding: 10px 17px;
	height:100%;
}

.chat-area {
	padding: 10px 17px;
	background-color: #ebebeb;
}

.marking {
	display: inline-block;
}
.marking.waiting {
	padding: 0 0 0 24px;
	color: #3ba8a7;
	background: url("../images/common/icon/mark_wait.png") 0 50% no-repeat;
	background-size: 20px 6px;
}
.marking.complete {
	padding: 0 0 0 24px;
	color: #db4e3f;
	background: url("../images/common/icon/mark_complete.png") 0 50% no-repeat;
	background-size: 15px 12px;
}

.qna_box {
	overflow: hidden;
	border-radius: 6px;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
	margin: 14px 0;
	background-color: #fff;
}
.qna_box dt {
	height: 45px;
	line-height: 45px;
	background: linear-gradient(#ffffff, #eff0f2);
}
.qna_box dt ul {
	display: -webkit-box;
}
.qna_box dt ul li {
	font-weight: bold;
	font-size: 12px;
	text-align: center;
	border-left: 1px solid #eaeaea;
}
.qna_box dt ul li.subject {
	width: 34%;
	padding: 0 0 0 16px;
	font-size: 16px;
	text-align: left;
	color: #3a89c9;
}
.qna_box dt ul li.progress {
	width: 37%;
}
.qna_box dt ul li.date {
	width: 29%;
	color: #505050;
}
.qna_box dd {
	padding: 15px;
	color: #787878;
	background-color: #fff;
	word-break: break-all;
}

.qna_box.new dd:after { position: relative; display: inline-block; content: 'N'; margin-left:10px; border-radius: 3px; background-color: #f46758; width: 20px; height: 20px; line-height: 20px; color: #fff; text-align: center; }

.qna_box.view dt ul li.subject {
	width: 71%;
}
.qna_box.view dt ul li.date {
	width: 29%;
}

.question_temp + .question_temp .write-date {
	display: block;
}
.question_temp .write-date {
	display: none;
	margin: 30px 0 10px 0;
	padding: 0 0 5px 0;
	border-bottom: 1px solid #d7d7d7;
	color: #505050;
	text-align: right;
	font-weight: bold;
	font-size: 12px;
}
.question_temp .q-stit {
	padding: 0 0 0 28px;
	color: #787878;
	background: url("../images/common/icon/ico_q.png") 0 0 no-repeat;
	background-size: 17.5px 20px;
	font-weight: bold;
}
.question_temp .group-btn {
	margin-top: 13px;
}
.question_temp .answer-box {
	margin-top: 17px;
	border: 1px solid #e6e6e6;
	border-radius: 6px;
	background-color: #f5f5f5;
}
.question_temp .answer-box .page-center.wait {
	min-height: 60px;
	color: #3ba8a7;
	font-size: 16px;
}
.question_temp .answer-box .page-center.wait :before {
	content: '';
	display: block;
	width: 20px;
	height: 6px;
	margin: 0 auto 6px auto;
	background: url("../images/common/icon/mark_wait.png") 0 0 no-repeat;
	background-size: 20px 6px;
}
.question_temp .answer-box .info * {
	vertical-align: middle;
}
.question_temp .answer-box .info {
	display: -webkit-box;
	-webkit-box-align: center;
	position: relative;
	height: 58px;
	border-bottom: 1px solid #d7d7d6;
	padding: 0 18px;
	font-size: 16px;
}
.question_temp .answer-box .info .thum {
	overflow: hidden;
	display: inline-block;
	margin: 0 6px 0 0;
	width: 39px;
	height: 39px;
	border-radius: 50%;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
}
.question_temp .answer-box .info .name * {
	vertical-align: baseline;
}
.question_temp .answer-box .info .name i {
	color: #00bf77;
}
.question_temp .answer-box .info .date {
	position: absolute;
	top: 21px;
	right: 18px;
	font-size: 12px;
	font-weight: bold;
	color: #505050;
}
.question_temp .answer-box .answer {
	padding: 21px;
	color: #000;
}
.question_temp .inquiry {
	margin-top: 15px;
}
.question_temp .inquiry .textarea {
	height: 190px !important;
}

/* 로그인 */
.login {
	/*
	display: -webkit-box;
	-webkit-box-pack: center;
	height: 100%;
	*/
	background-color: #f9fafa;
}
.login section {width: 100%; padding: 35px 35px;}
.login section .member-intro * {vertical-align: baseline;}
.login section .member-intro {margin-bottom: 20px; text-align: center; color: #e4e4e5; line-height: 1;}
.login section .member-intro a {color: #999;}
.login section .member-intro a:hover {color: #f46758;}

.login ul li {margin: 5px 0;}
.control-login {
	width: 100%;
	padding: 0 16px;
	height: 36px;
	line-height: 36px;
	border-radius: 6px;
	padding: 0 8px;
	border: 2px solid #f46758;
	background-color: #fff;
	color: #f46758;
	font-size: 18px;
}
.login .control-login::-webkit-input-placeholder {
	color: #f46758;
}
.login .chk-area * {
	vertical-align: middle;
}
.login .chk-area {
	margin-top: 25px;
	text-align: center;
}
.login .chk-area label {
	font-size: 16px;
}
.login .group-btn {
	margin-top: 12px;
}
.login .full-logo {
	margin-top: 42px;
	padding: 0 0 14px 0;
	text-align: center;
	border-bottom: 1px solid #c5c5c6;
	box-shadow: inset 0 -1px 0 0  #9e9f9f;
	text-align: center;
}
.login .full-logo img {
	vertical-align: top;
}
.login .desc {
	padding: 17px 0 0 0;
	text-shadow: 0 0 0;
	line-height: 1.4;
	font-size: 19px;
	text-align: center;
}

.login .desc strong{font-weight: normal;}

.login .desc .app {
	color: #f46758;
}

h3.stit {
	display: inline-block;
	height: 32px;
	line-height: 32px;
	margin: 12px 0 -1px 5px;
	padding: 0 25px;
	border-radius: 10px 10px 0 0;
	border: 1px solid #c61300;
	border-bottom: none;
	text-align: center;
	color: #fff;
	font-size: 15px;
	background: linear-gradient(#f46757, #da4d3e);
}

h3.stit + .group_box {
	margin-top: 0;
}

.group_box {
	overflow: hidden;
	margin-top: 12px;
	border: 1px solid #c3dfed;
	border-radius: 6px;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
	background-color: #fff;
}
.group_box h4 {
	height: 48px;
	line-height: 48px;
	background: linear-gradient(#e8fdff, #c9efff);
	padding: 0 0 0 12px;
	border-bottom: 1px solid #c3dfed;
	font-size: 15px;
	color: #000;
}
.group_box .cnt {
	position: relative;
	padding: 32px 0px 28px 0px;
}
.group_box .cnt.pt_none {
	padding-top: 0;
}
.group_box .cnt.pd_none {
	padding: 0;
}
.group_box .score-col {
	position: relative;
	margin: 0 0 25px 0;
	border-bottom: 1px solid #dfeef5;
}
.group_box .score-col strong {
	font-size: 36px;
}
.group_box .score-col .total * {
	vertical-align: middle;
}
.group_box .score-col .total {
	position: absolute;
	top: 10px;
	left: 14px;
}
.group_box .score-col .total strong {
	font-size: 24px;
}

.group_box .tab {
	background-color: #8394ca;
}
.group_box .tab ul {
	display: -webkit-box;
	-webkit-box-align: end;
	height: 33px;
	line-height: 33px;
}
.group_box .tab ul li {
	-webkit-box-flex: 1;
	height: 29px;
	line-height: 29px;
	text-align: center;
}
.group_box .tab ul li.on {
	background: linear-gradient(#e8fdff, #d2f3ff);
	border-radius: 6px 6px 0 0;
	box-shadow: inset 0 2px 2px 0 #fff;
}

.group_box .tab ul li a {
	display: block;
}

.group_box .tab ul li.on a {
	color: #000;
}

/* 통계 교과 탭 */
.subject-tab {
	overflow: hidden;
}
.subject-tab li {
	float: left;
	width: calc(100%/7);
	padding: 0 2px;
}
.subject-tab li a {
	display: block;
	height: 28px;
	line-height: 28px;
	border-radius: 3px;
	border: 1px solid #2099d5;
	color: #20bfd5;
	text-align: center;
	font-size: 15px;
	background-color: #fff;
}
.subject-tab li a.on {
	color: #fff;
	background-color: #20bfd5;
}

/* 통계 타입 */
.align-both {
	display: -webkit-box;
	-webkit-box-pack: center;
	-webkit-box-align: center;
}

.score-box {
	margin: 0 10px;
	text-align: center;
}
.score-box dt i {
	font-size: 36px;
	font-weight: bold;
}
.score-box dt p {
	font-size: 14px;
	color: #000;
}
.score-box dd {
	font-size: 0;
}
.score-box dd i {
	margin: 0 1px;
}
.score-box.danger dt i {
	color: #f46758;
}
.score-box.active dt i {
	color: #2eb8c5;
}

i.star {
	display: inline-block;
	width: 20px;
	height: 20px;
	background: url("../images/statistic/star.png") right 0 no-repeat;
	background-size: cover;
}
i.star.on {
	background-position: 0 0;
}

/* 그래프 타입 */
.division {
	position: absolute;
	top: 5px;
	right: 15px;
	text-align: right;
}
.division span * {
	vertical-align: middle;
}
.division span {
	display: inline-block;
	padding: 0 0 0 10px;
	font-size: 12px;
	color: #000;
}
.division span:before {
	position: relative;
	content: '';
	top: 1px;
	margin: 0 4px 0 0;
	display: inline-block;
	width: 10px;
	height: 10px;
	background-color: red;
}
.division span.active:before {
	background-color: #2eb8c5;
}
.division span.danger:before {
	background-color: #f46758;
}

.grp-result-type .grp-donut {
	margin-right: 10%;
}

.grp-donut {
	position: relative;
	width: 78px;
	height: 78px;
	background: url("../images/statistic/donut_bg.png") 0 0 no-repeat;
	background-size: cover;
}
.grp-donut dl {
	line-height: 1;
}
.grp-donut dl dt {
	font-size: 13px;
	color: #51748f;
}
.grp-donut dl dd {
	font-weight: bold;
	font-size: 14px;
}
.grp-donut dl dd i {
	font-size: 30px;
	color: #2eb8c5;
}

.grp-bar {
	width: 105px;
	margin: 0 3%;
}
.grp-bar strong {
	display: block;
	text-align: center;
	font-weight: normal;
	line-height: 1;
}

.grp-line {
	width: 244px;
}

.grp-result li * {
	vertical-align: baseline;
}
.grp-result li {
	margin: 5px 0;
	text-align: right;
}
.grp-result li b {
	display: inline-block;
	width: 45px;
	text-align: right;
}
.grp-result li.rate i {
	font-size: 30px;
}

.rank * {
	vertical-align: middle;
}

.rank {
	padding: 0 0 0 0;
}
.rank img {
	width: 42px;
	margin: 0 13px 0 0;
}
.rank strong i {
	font-size: 28px;
}

/* 통계 테이블 */
.tbl_chart table {
	table-layout: fixed;
	width: 100%;
	font-size: 12px;
}
.tbl_chart table thead th:first-child {
	text-align: left;
	padding: 0 0 0 20px;
}
.tbl_chart table tbody td:first-child {
	text-align: left;
	padding: 0 0 0 20px;
}
.tbl_chart table thead th {
	height: 40px;
	border-bottom: 2px solid #e6e6e6;
}
.tbl_chart table tbody td {
	height: 36px;
	border-top: 1px solid #e6e6e6;
	text-align: center;
}
.tbl_chart table.left-type th, .tbl_chart table.left-type td {
	padding: 0 0 0 20px;
	text-align: left;
}

.time-table {
	padding: 30px 9px 0;
}

.subject-table .month {
	margin-bottom: 10px;
	text-align: center;
	color: #000;
	font-size: 30px;
}

.subject-table .week {
	border-radius: 13px;
	margin-bottom: 7px;
	text-align: center;
	background-color: #71bee5;
	display: -webkit-box;
	-webkit-box-pack: center;
}
.subject-table .week li {
	height: 45px;
	line-height: 45px;
	color: #fff;
	font-size: 21px;
}

.subject-table .schedule {
	overflow: hidden;
	border: 1px solid #bcbcbc;
	border-radius: 12px;
	background-color: #71bee5;
}

.subject-table .schedule table {
	border-collapse: collapse;
	width: 100%;
}
.subject-table .schedule table tr:first-child th, .subject-table .schedule table tr:first-child td {
	padding: 4px;
	box-shadow: none;
}
.subject-table .schedule table tr th,
.subject-table .schedule table tr td {
	height: 45px;
	line-height: 45px;
	font-size: 16px;
	text-align: center;
	border: 1px solid #60adda;
	box-shadow: inset 0 1px 0 0 rgba(255, 255, 255, 0.2);
	color: #000;
}
.subject-table .schedule table tr th {
	color: #fff;
}

.subject-table .schedule table tr td p {
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
}

