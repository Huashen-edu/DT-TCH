@charset "UTF-8";
#wrapper {position: relative; min-height: 100%;transition:0.7s;}
#ctsDrawerMenu {transition:0.7s;}
#content_container {position: relative; width:100%; height: 100%;}


.top {
	position: fixed;
	bottom: 15px;
	right: 5px;
	display: block;
	width: 35px;
	height: 35px;
	border-radius: 6px;
	border: 1px solid #eaeaea;
	background: rgba(255, 255, 255, 0.4) url("../images/common/top.png") center center no-repeat;
	background-size: 24px 15px;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
}


.intro {width: 262px;}

/* 공지사항 */
section.notice dl:after {
	content: '';
	display: block;
	/*
	height: 0;
	visibility: hidden;
	*/
	clear: both;
}

section.notice dl dt {
	position: relative;
	color: #787878;
	box-shadow: 0 1px 10px 0 rgba(0, 0, 0, 0.2);
	background: #fff;
}

section.notice dl dt mark {
	float: left;
	margin-right: 10px;
	width: 85px;
	height: 89px;
	line-height: 89px;
	font-size: 18px;
	color: #fff;
	text-align: center;
	font-weight: bold;
	background-size: cover;
}

section.notice dl dt mark.org {background: url("../images/notice_mark1.jpg") 0 0 no-repeat;}
section.notice dl dt mark.tch {background: url("../images/notice_mark2.jpg") 0 0 no-repeat;}

section.notice dl dt h3 {
	display: flex;
	display: -webkit-flex;
    flex: 1;
	-webkit-flex: 1;
	padding: 13px 0 7px 0;
	font-size: 17px;
	color: #333;
	line-height: 1.2;
	font-weight: bold;
}

section.notice dl dt h3 nn.new:after {
	content: 'N';
	position: relative;
	top: -2px;
	display: inline-block;
	margin-left:10px;
	border-radius: 3px;
	background-color: #f46758;
	width: 16px;
	height: 16px;
	line-height: 16px;
	color: #fff;
	text-align: center;
	font-size: 12px;
}

section.notice dl dt h3 span.na_txt {display:inline-block; max-width:80%;}

section.notice dl dt ff.file {
	position: relative;
	top: 2px;
    left: 8px;
    display: inline-block;
    background: url("../images/icon/clip.png") no-repeat;
    width: 13px;
    height: 14px;
}

section.notice dl dt p,
section.notice dl dt i {font-size: 13px;}

section.notice dl dt p {
	width: 65%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

section.notice dl dt:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}
section.notice dl dd {
	display: none;
	padding: 10px;
	background-color: #f5f5f5;
	word-break: break-all;
}

section.notice dl dd.on {display: block;}

/* 문의 답변 */
.msg-box {
	text-align: center;
	padding: 100px 0;
	font-size: 18px;
	color: #f46758;
}

.btn-faq {
	height: 35px;
	line-height: 35px;
	padding: 0 8px 0 12px;
	/*padding: 0 8px 0 32px;*/
	/*background: #dd503f url('../images/common/icon/ico_add.png') 5px center no-repeat;*/
	-webkit-background-size: 23.5px 25px;
	color: #505050;
    border: 1px solid #ccc;
    text-align: center;
    background-color: #f0f2f3;

}


.select-area {padding: 15px 17px 25px; box-shadow: inset 0 -2px 10px 0px rgba(0, 0, 0, 0.2);}
.select-area .desc {font-size: 16px;}
.select-area .tch-list {
	overflow: hidden;
	width: 100%;
	margin-top: 10px;
	margin-bottom: 5px;
}

.select-area .tch-list ul {margin: 0 0 0 -4px;}
.select-area .tch-list ul li {
	float: left;
	width: 50%;
	margin: 0 0 5px 0;
	padding: 0 4px;
}

.select-area .tch-list ul li a {
	display: block;
	height: 26px;
	line-height: 26px;
	border-radius: 6px;
	text-align: center;
	color: #fff;
	font-size: 12px;
	color: #666;
	border: 1px solid #cccccc;
	background: #fff;
	/*background: #f0f2f3;*/
}

.select-area .tch-list ul li a.danger {
	background-color: #f56859;
	border: 1px solid #e05142;
	color: #fff;
}

/* 아빠 */
#wrapper[data-color="D"] .select-area .tch-list ul li a.danger {
	background-color: #1a1af4;
	border: 1px solid #090ce5;
}
/* 엄마 */
#wrapper[data-color="M"] .select-area .tch-list ul li a.danger {
	background-color: #f56859;
	border: 1px solid #e05142;
}
/* 기타 */
#wrapper[data-color="E"] .select-area .tch-list ul li a.danger {
	background-color: #02cc42;
	border: 1px solid #05ac38;
}

.select-area + .group-btn {padding: 20px 17px; }

.pr_edit {padding: 12px 0; background-color: #f0f0f0;}
.pr_edit .user * {vertical-align: middle;}
.pr_edit .user {
	display: -webkit-box; -webkit-box-align: center; -webkit-box-pack: center;
	padding: 11px 0; color: #000;
}
.pr_edit .user .thum {position: relative; width: 102px; height: 102px; border-radius: 50%; box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);}
.pr_edit .user .thum span {display: block; width: 102px; height: 102px; border-radius: 50%; overflow: hidden;}
.pr_edit .user .thum .shutter {
	position: absolute; top: 74px; right: 0; width: 30px; height: 30px;
	border-radius: 50%;
	box-shadow: 1px 1px 0 0 rgba(0, 0, 0, 0.2);
	background: #fff url('../images/icon/ico_camera.png') 50% 50% no-repeat;
	background-size: 70%;
}

.pr_edit .user dl {overflow: hidden;}
.pr_edit .user dl dt {text-align: center;}
.pr_edit .user dl dt strong {font-size: 27px; }
.pr_edit .user dl dt p * {vertical-align: baseline; }
.pr_edit .user dl dt p {
	margin: 0 0 0 0;
	color: #9090c5;
	font-size: 12px;
	font-weight: bold;
}

.pr_edit .user dl dt p span {color: #000;}
.pr_edit .user dl dd {margin-top: 15px; text-align: center;}
.pr_edit .user dl dd .btn-block {
	font-size: 12px;
	background-color: #fff;
}

.pr_edit .user:after {
	content: '';
	display: block;
	height: 0;
	visibility: hidden;
	clear: both;
}

.pr_edit .info {
	margin: 0 15px;
	padding: 10px 13px;
	border-radius: 6px;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
	background-color: #fff;
}

.pr_edit .info ul li {
	width: 100%;
	padding: 4px 0;
	display: table;
}

.pr_edit .info ul li strong,
.pr_edit .info ul li p {display: table-cell; vertical-align: middle;}

.pr_edit .info ul li strong {
	padding: 0 9px 0 0;
	font-size: 12px;
	text-align: right;
	width: 100px;
	color: #505050;
}

.pr_edit .info ul li p {font-size: 14px; word-break: break-all;}
.pr_edit .info ul li p input[type="text"].control {width: 100%;}
.pr_edit .info ul li p input[type="text"].control.birthday {width: 22%;}
.pr_edit .info ul li p input[type="text"].control.email {width: 43%;}
.pr_edit .info ul li p input[type="phone"].control.phone {width: 28%;}
.pr_edit .info ul li p input[type="password"].control {width: 100%;}
.pr_edit .group-btn {margin: 12px 0 0 0;}
.pr_edit .group-btn .btn-round.edit:before {
	position: relative;
	top: 5px;
	margin-right: 5px;
	content: '';
	display: inline-block;
	width: 24px;
	height: 24px;
	background: url("../images/common/icon/ico_set.png") 0 0 no-repeat;
	background-size: 24px 24px;
}

.pr_edit .edit_profile_img {
	float: left;
    overflow: hidden;
    display: inline-block;
    margin-right: 12px;
    width: 102px;
    height: 102px;
    border-radius: 50%;
    box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
}

/* 통계 */
.statistic {padding: 10px 14px; background-color: #f0f0f0;}

/* 말줄임 표시 */
.cts_ellipsis {text-overflow: ellipsis; white-space: nowrap; overflow: hidden;}
.noticeHeader {cursor:pointer;}

/* 댓글 날짜 */
.add_reply_date {
	height: 33px;
	padding: 15px 19px 28px 0;
	text-align: right;
	font-size: 12px;
	font-weight: bold;
	color: #505050;
}

/* spinner */
/* spinner */
.pageLoadwrap {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	vertical-align: middle;
	min-height: 100%;
	text-align: center;
}
.pageLoadwrap .center { position: absolute; top: 50%; left: 50%; margin: 0 0 0 0; }
.pageLoadwrap .loader { font-size: 14px; margin: -0.5em -0.5em; width: 1em; height: 1em; border-radius: 50%; position: relative; text-indent: -9999em; -webkit-animation: load4 1.3s infinite linear; animation: load4 1.3s infinite linear; }



.loadwrap {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	vertical-align: middle;
	min-height: 100%;
	background-color:#FFFFFF;
}
.loadwrap .center { position: absolute; top: 50%; left: 50%; margin: 0 0 0 0; }
.loadwrap .loader { font-size: 14px; margin: -0.5em -0.5em; width: 1em; height: 1em; border-radius: 50%; position: relative; text-indent: -9999em; -webkit-animation: load4 1.3s infinite linear; animation: load4 1.3s infinite linear; }

@-webkit-keyframes load4 {
	0%,
	100% { box-shadow: 0em -3em 0em 0.2em #000000, 2em -2em 0 0em #000000, 3em 0em 0 -0.5em #000000, 2em 2em 0 -0.5em #000000, 0em 3em 0 -0.5em #000000, -2em 2em 0 -0.5em #000000, -3em 0em 0 -0.5em #000000, -2em -2em 0 0em #000000; }
	12.5% { box-shadow: 0em -3em 0em 0em #000000, 2em -2em 0 0.2em #000000, 3em 0em 0 0em #000000, 2em 2em 0 -0.5em #000000, 0em 3em 0 -0.5em #000000, -2em 2em 0 -0.5em #000000, -3em 0em 0 -0.5em #000000, -2em -2em 0 -0.5em #000000; }
	25% { box-shadow: 0em -3em 0em -0.5em #000000, 2em -2em 0 0em #000000, 3em 0em 0 0.2em #000000, 2em 2em 0 0em #000000, 0em 3em 0 -0.5em #000000, -2em 2em 0 -0.5em #000000, -3em 0em 0 -0.5em #000000, -2em -2em 0 -0.5em #000000; }
	37.5% { box-shadow: 0em -3em 0em -0.5em #000000, 2em -2em 0 -0.5em #000000, 3em 0em 0 0em #000000, 2em 2em 0 0.2em #000000, 0em 3em 0 0em #000000, -2em 2em 0 -0.5em #000000, -3em 0em 0 -0.5em #000000, -2em -2em 0 -0.5em #000000; }
	50% { box-shadow: 0em -3em 0em -0.5em #000000, 2em -2em 0 -0.5em #000000, 3em 0em 0 -0.5em #000000, 2em 2em 0 0em #000000, 0em 3em 0 0.2em #000000, -2em 2em 0 0em #000000, -3em 0em 0 -0.5em #000000, -2em -2em 0 -0.5em #000000; }
	62.5% { box-shadow: 0em -3em 0em -0.5em #000000, 2em -2em 0 -0.5em #000000, 3em 0em 0 -0.5em #000000, 2em 2em 0 -0.5em #000000, 0em 3em 0 0em #000000, -2em 2em 0 0.2em #000000, -3em 0em 0 0em #000000, -2em -2em 0 -0.5em #000000; }
	75% { box-shadow: 0em -3em 0em -0.5em #000000, 2em -2em 0 -0.5em #000000, 3em 0em 0 -0.5em #000000, 2em 2em 0 -0.5em #000000, 0em 3em 0 -0.5em #000000, -2em 2em 0 0em #000000, -3em 0em 0 0.2em #000000, -2em -2em 0 0em #000000; }
	87.5% { box-shadow: 0em -3em 0em 0em #000000, 2em -2em 0 -0.5em #000000, 3em 0em 0 -0.5em #000000, 2em 2em 0 -0.5em #000000, 0em 3em 0 -0.5em #000000, -2em 2em 0 0em #000000, -3em 0em 0 0em #000000, -2em -2em 0 0.2em #000000; }

}
@keyframes load4 {
	0%,
	100% { box-shadow: 0em -3em 0em 0.2em #000000, 2em -2em 0 0em #000000, 3em 0em 0 -0.5em #000000, 2em 2em 0 -0.5em #000000, 0em 3em 0 -0.5em #000000, -2em 2em 0 -0.5em #000000, -3em 0em 0 -0.5em #000000, -2em -2em 0 0em #000000; }
