/* Let's get this party started */
::-webkit-scrollbar {width: 7px;}

/* Track */
::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	-webkit-border-radius: 10px;
	border-radius: 10px;
}

/* Handle */
::-webkit-scrollbar-thumb {
	-webkit-border-radius: 10px;
	border-radius: 10px;
	background: rgba(174,175,177,0.8);
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.5);
}
::-webkit-scrollbar-thumb:window-inactive {
	background: rgba(174,175,177,0.4);
}

a {cursor : pointer;}




/*     통계       */

.group_select * {vertical-align: middle;}
.group_select {
	position: relative;
	padding: 0 0 20px 0;
	text-align: right;
	overflow: hidden;
	margin-top: 12px;
	border: 1px solid #c3dfed;
	border-radius: 6px;
	box-shadow: 2px 2px 0 0 rgba(0, 0, 0, 0.2);
	background-color: #fff;
}
.group_select h4 {
	height: 48px;
	line-height: 48px;
	background: linear-gradient(#e8fdff, #c9efff);
	padding: 0 20px;
	border-bottom: 1px solid #c3dfed;
	font-size: 15px;
	color: #000;
}
.group_select .form-control {width: auto; display: inline-block;}
.group_select .group {float: left;}

.chart_icon {font-size: 0;}
.chart_icon a {
	display: inline-block; width: 26px; height: 25px; margin: 0 5px 0 0;
	background-image: url('../images/statistic/ico_type.gif'); background-repeat: no-repeat;
	-webkit-filter: grayscale(100%);
}
.chart_icon a.type1 {background-position: calc(-30px*0) 0;}
.chart_icon a.type2 {background-position: calc(-30px*1) 0;}
.chart_icon a.type3 {background-position: calc(-30px*2) 0;}
.chart_icon a.type4 {background-position: calc(-30px*3) 0;}
.chart_icon a:hover,
.chart_icon a.on {-webkit-filter: grayscale(0%);}

.graph_area {
	overflow: hidden;
	margin: 20px;
	border-radius: 6px;
	border: 2px solid #446cb3;
}

/*   통계 select box    */
#selectLayer select {margin-right:10px;}
.selectLayer {float: left;}

.graph_area .injection {position:relative;  padding: 30px;}
.graph_area  .injection > div {visibility: hidden; height: 0;}


.statistic .btn_search{
	height: 30px;
	line-height: 30px;
	border-radius: 10px;
}

.sizeAuto { visibility:visible !important; height:auto !important;}

