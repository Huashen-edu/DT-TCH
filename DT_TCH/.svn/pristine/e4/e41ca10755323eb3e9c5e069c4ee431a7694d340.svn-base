cts-fisrt-view{
	position:absolute;
	display:block;
	width:100%;
	height:100%;
	background: white;
	top:0;
	left:0;
	display:flex;
	align-items: center;
	text-align: center;
	z-index: 9;
}
cts-fisrt-view > *{
	flex:1;
	display:flex;
	text-align: center;
	flex-direction: column;
}
cts-loading{
	position:absolute;
	display:block;
	width:100%;
	height:100%;
	background:rgba(255,255,255,.7);
	top:0;
	left:0;
	display:flex;
	align-items: center;
	text-align: center;
	 z-index: 9;
}
/* Loading */
@keyframes ctsloadingrotate{
	0% {-webkit-transform: rotate(0deg) translate3d(0,0,0)}
	100% {-webkit-transform: rotate(359deg) translate3d(0,0,0)}
}
@-webkit-keyframes ctsloadingrotate {
	0% {-webkit-transform: rotate(0deg) translate3d(0,0,0)}
	100% {-webkit-transform: rotate(359deg) translate3d(0,0,0)}
}


button{
			border:1px solid #ccc;
			padding:3px;
		}


		.cts-r-containor{
			position:relative;
			width: 100%;
			height: 650px;
			display:flex;
			flex-direction: column;
			box-sizing:border-box;
		}

		.cts-r-header{
			flex:none;
			display: flex;
  			align-items: center;
			height:75px;
		}

		.cts-r-header:after{
			display:block;
			content:"";
			clear:both;
		}

		.cts-r-header .cts-r-h-box-area{
			float:left;
		}

		.cts-r-header .cts-r-h-box-area > div{
			display:inline-block;
		}

		.cts-r-header .cts-r-h-button-area{
			float:right;
			flex:1;
			text-align:right;
		}

		.cts-r-content{
			position: relative;
			flex:1;
			display:flex;
			border: 3px solid #ccc;
			border-radius: 15px;
			padding:15px;
		}

		.cts-r-content .cts-content-chart-area{
			flex:1.4;
			display:flex;
			flex-direction: column;
		}

		.chart_icon {font-size: 0;}
		.chart_icon a {
			display: inline-block; width: 26px; height: 25px; margin: 0 5px 0 0;
			background-image: url('./ico_type.gif'); background-repeat: no-repeat;
			-webkit-filter: grayscale(100%);
			display:none;
		}
		.chart_icon a[cts-chart-build=bar] {background-position: calc(-30px*0) 0;}
		.chart_icon a[cts-chart-build=line] {background-position: calc(-30px*1) 0;}
		.chart_icon a[cts-chart-build=spider] {background-position: calc(-30px*2) 0;}
		.chart_icon a[cts-chart-build=pie] {background-position: calc(-30px*3) 0;}
		.chart_icon a:hover,
		.chart_icon a.on {-webkit-filter: grayscale(0%);}

		/*button[cts-chart-build]{
			display:none;
			color: #fff;
			background-color: #ccc;
			border-color: #333;
		}
		button[cts-chart-build].on{
			background-color: black;
			color: white;
		}*/
		.cts-r-content .cts-content-chart-area{
			margin-right: 30px;
		}
		.cts-r-content .cts-content-chart-area > div{
			flex:1;
			position:relative;
		}
		.cts-r-content .cts-content-chart-area > .cts-r-h-button-area {
			flex:none;
			text-align: right;
		}

		.cts-r-content .cts-content-table-area{
			flex:.6;
			overflow-y: auto;
			overflow-x: hidden;
			border : 1px solid #e2e2e2;
			border-radius: 10px;
		}

		[diffarea] .colorLabel{
			display:inline-block;
			width:30px;
			height:30px;
			border-radius: 15px;
			top: 15px;
			position: relative;
			margin-left:20px;
			margin-right:5px;
		}
		[diffarea] .colorLabel > span:first-child{
			content: "";
			position:absolute;
			width:30px;
			height:4px;
			top:13px;
			left:0;
		}
		[diffarea] .colorLabel > span:last-child{
			content: "";
			width:15px;
			height:15px;
			border-radius: 15px;
			position:absolute;
			top: calc(15px - 7px);
			left: calc(15px - 7px);
		}
		table.cts-content-table { 
			width: 100%; margin: -1px 0 0 0; font-size:13px; 
		}
		table.cts-content-table th,
		table.cts-content-table td {
			border-top: 1px solid #e2e2e2; text-align: center; position: relative; 
		}
		table.cts-content-table.getPhysicalTestStatTable{
			margin-bottom:25px;
			font-size:12px !important;
		}

		table.cts-content-table.getPhysicalTestStatTable th,
		table.cts-content-table.getPhysicalTestStatTable td {
			border-bottom: 1px solid #e2e2e2; text-align: center; position: relative;
		}
		
		table.cts-content-table th, table.cts-content-table td  {
			height: 34px; vertical-align: middle; 
		}
		table.cts-content-table th{ 
			background-color:#f0f0f0;
			font-weight: bold; 
		}
		table.cts-content-table th.cts-content-table-no{
			width:30px; 
		}
		table.cts-content-table .cts-color-label{
			display:inline-block;
			width:11px;height:11px;
			border-radius:5px;
			border:1px solid rgba(255,255,255,1);
			background:#eaeaea;
		}
		table.cts-content-table .ct-data-summary-tr td,table.cts-content-table .ct-data-summary-tr th{
			background : #F0F0F0;
		}
		table.cts-content-table .ct-data-summary-tr .cts-color-label {
			height:2px;
			border:none;
		}
		/* 활성화 시 */ 
		table.cts-content-table .active td {
			background: #ffffcc;
			color: black;
		}


		[cts-table-sort]:after{
			display: inline;
			content : "↕";
			color: gray;
		}
		[cts-table-sort=up]:after{
			content:"↑";
			color : red;
		}
		[cts-table-sort=down]:after{
			content:"↓";
			color : blue;
		}
		.getPhysicalTestStatHeader{
			padding : 15px 0;
			text-align: center;
			font-size : 15px;
			font-weight: bold;
		}
		.getPhysicalTestStatHeader td{
			vertical-align: middle;
			text-align: center;
		}

		/*Component*/
		button{
			width:auto;
			display: inline-block;
			padding: 6px 12px;
			margin-bottom: 0;
			font-size: 14px;
			font-weight: normal;
			line-height: 1.42857143;
			text-align: center;
			white-space: nowrap;
			vertical-align: middle;
			-ms-touch-action: manipulation;
			touch-action: manipulation;
			cursor: pointer;
			-webkit-user-select: none;
			-moz-user-select: none;
			-ms-user-select: none;
			user-select: none;
			background-image: none;
			border: 1px solid transparent;
			border-radius: 0px;
		}
		button[cts-build-report]{
			width: 100px;
			color: #fff;
			background-color: #d9534f;
			border-color: #d43f3a;
		}

		.form-control {
			margin-right: 10px;
			display: inline-block;
			width: auto;
			height: 34px;
			padding: 6px 12px;
			font-size: 14px;
			line-height: 1.42857143;
			color: #555;
			background-color: #fff;
			background-image: none;
			border: 1px solid #ccc;
			border-radius: 0px;
			-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
			box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
			-webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
			-o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
			transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
		}



