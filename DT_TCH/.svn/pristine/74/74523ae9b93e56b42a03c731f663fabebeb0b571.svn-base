  </head>
  <body style="background-color: #f9fafa;">

 <div id="wrapper" class="page-center">
      <div class="intro" style="display: none;">
        <img src="../images/load_015s.gif" alt="" />
      </div>
      
      <!-- contain -->
       <div id="container" style="display: none;">

        <!-- 编码区 -->
        <div class="login">
          <section>
            <div class="full-logo" style ="margin-bottom: 30px;">
              <img src="../images/common/login_logo.png" alt="DigitalTextbook"  />
            </div>
            <ul style="padding-left:0;margin: 0;">
             <li>
              <i class="am-icon-user am-icon-sm am-icon-fw" style="color: #e15531">
              </i>
              <!--<img src="../images/common/btn/btn_username.png" alt="DigitalTextbook"  />-->
              <input type="text" name=""  id="user_name" value=""
               data-kor="USER_ID"style="width:80%;border:0px;background-color: #f9fafa;margin-left:10px;"/>
              <div style="height:1px;width:100%;background:#9e9f9f;margin:5px 0 10px 0;"/>
            </li>
            
              <li>
              <i class="am-icon-lock am-icon-md am-icon-fw" style="color: #e15531">
              </i>
              <!--<img src="../images/common/btn/btn_username.png" alt="DigitalTextbook"  />-->
              	<input 
                type="password"
                name="pass"
                class="control-login"
                placeholder="请输入密码"
                data-kor="PASSWORD"style="width:80%;border:0px;background-color: #f9fafa;margin-left:10px;"
            />
              <div style="height:1px;width:100%;background:#9e9f9f;margin:5px 0 10px 0;"/>
            </li>
            <li>
            <i class="am-icon-lock am-icon-md am-icon-fw" style="color: #e15531">
            </i>
            <!-- <img src="../images/common/btn/btn_password.png" alt="DigitalTextbook"  />-->
      <input 
                type="password"
                name="repass"
                class="control-login"
                placeholder="请再次输入密码"
                data-kor="REPASSWORD"style="width:80%;border:0px;background-color: #f9fafa;margin-left:10px;"
            />
            <div style="height:1px;width:100%;background:#9e9f9f;margin:5px 0 10px 0;"/>
          </li>
        </ul>

        <div class="group-btn">
        
   
	<div class="group-btn">
		<button href="javascript:void(0)"  id=btnNext class="btn-round danger btn_edit_parent" data-kor="保存">保存修改</button>
	</div>
	
             <!-- alert -->
    <div class="alert" id="main_common_alert" style="display: none;">
    
    </div>
        </div>
        <p class="desc" data-kor="欢迎进入暖心妈妈微信端" style="padding:0;font-size:14px;">
          <strong>
            欢迎使用
          </strong>
          <strong class="app">
            暖心微信端
          </strong>
        </p>
     
      </section>


    </div>
  </div>
</div>
