pages.p_chart_type = function(){

	try{
		$('#gnb').show();
	    //리스트 메이커
	    //pages.p_chart_type();
		 var pageParam = c_util.getUrlParam() ,
		    pname = pageParam.pname ;
		//make pageMap
		c_util.pageMap();
		//make lnb
		c_util.subLnb();

		reportModule();
	} catch (e) {
		console.error("run_report",e);
	}

	var jsList = ["./req/highstock.js","./req/highcharts-more.js","./req/modules/exporting.js","./req/chart_report_var.js","./req/chart_report_u.js","./req/chart_report_m.js"];
	var jsListLen = jsList.length;
	var successCnt = 0;

	jsList.forEach(function(url, index){
		$.getScript(url ,  function(){
			successCnt++;
			if(successCnt == jsListLen){
				console.info("모두 완료!!")
				_a.init( report_mode , cts.Chart.codeList[report_mode] );
			}
			console.info("총 : " + jsListLen + "개중 완료" , successCnt);
		});
	});

};