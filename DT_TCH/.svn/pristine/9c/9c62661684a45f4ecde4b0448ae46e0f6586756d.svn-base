/* 과목 머릿말 */
mark[class^="mark_"] { display: inline-block; line-height: 22px; padding: 0 8px; }
mark[class^="mark_"][class*="sci"] { background: #ddefe5; }
mark[class^="mark_"].teacher { background-color: #dae2f3; }
mark[class^="mark_"].student { background-color: #ddefe5; }

/* 라운드 버튼: ex)목록으로(하늘색)*/
.btn_round { display: inline-block; height: 24px; line-height: 24px; padding: 0 12px; border-radius: 5px; border: 1px solid #51b9f1; color: #51b9f1; }
.btn_round * { vertical-align: middle; }
.btn_round.default { color: #78828c; border-color: #ccc; }
.btn_round.defaultF { background-color: #ebebeb; color: #525b6f; border-color: #ccc; }
.btn_round.defaultF.reverse { background-color: #fff; }
.btn_round.danger { color: #ec4a3f; border-color: #ec4a3f; background-color: #fff; }
.btn_round i { display: inline-block; width: 14px; height: 14px; margin: -4px 4px 0 0; background: url() 50% 50% no-repeat; }
.btn_round i.add { background-image: url("../images/common/icon/ico_cross.png"); }
.btn_round i.x { background-image: url("../images/common/icon/ico_x.png"); }

/* 3개 노출되는 셀렉트박스 */
.select_row { position: relative; }
.select_row p { border: 1px solid #e1e1e1; line-height: 28px; padding: 0 6px 0 30px; background: url("../images/common/icon/ico_arrow1.png") 8px 50% no-repeat; }
.select_row .layer { position: absolute; overflow: hidden; overflow-y: auto; width: 100%; left: 0; top: -28px; border: 1px solid #e1e1e1; height: 84px; background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#ECECEC), to(#ECECEC), color-stop(0.5, #FFFFFF)); }
.select_row .layer ul li { padding: 0 0 0 30px; line-height: 28px; color: #525b6f; cursor: pointer; }
.select_row .layer ul li.checked { background: url("../images/common/icon/ico_arrow1.png") 8px 50% no-repeat; }